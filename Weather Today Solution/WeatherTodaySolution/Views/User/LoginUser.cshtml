
@{
    ViewBag.Title = "LoginUser";
}

<h2>Login User</h2>
@using (Html.BeginForm())
{
    <table>
        <tr>
            <td>Email</td>
            <td>@Html.TextBox("uEmail")</td>
        </tr>
        <tr>
            <td>
                Password
            </td>
            <td>
                @Html.Password("uPass")
            </td>
        </tr>
        <tr>
            <td><input class="btn btn-primary" type="submit" value="Login"></td>
        </tr>
        
    </table>
    <b style="color:red">@ViewBag.Msg</b>
}

